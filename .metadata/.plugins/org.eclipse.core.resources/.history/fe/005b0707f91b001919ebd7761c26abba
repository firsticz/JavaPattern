
public class TerminalExpression {
	private String data; 
	 
	   public TerminalExpression(String data){ 
	      this.data = data;  
	   } 
	 
	   @Override 
	   public boolean interpret(String context) { 
	      if(context.contains(data)){ 
	         return true; 
	      } 
	      return false; 
	   }
}
