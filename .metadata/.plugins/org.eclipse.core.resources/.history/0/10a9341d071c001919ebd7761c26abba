
public class ProxyVDO implements Image{

	private RealVDO realImage; 
	   private String fileName; 
	 
	   public ProxyVDO(String fileName){ 
	      this.fileName = fileName; 
	   }
	   @Override
	   public void display() { 
		      if(realImage == null){ 
		         realImage = new RealVDO(fileName); 
		      } 
		      realImage.display(); 
		   }
}
